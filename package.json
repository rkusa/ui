    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "next": "^10.0.0",
    "prettier": "^2.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "typescript": "^4.0.2"
